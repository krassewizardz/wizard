<BorderPane prefHeight="500" prefWidth="960" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wizard.controllers.OverviewController">
    <top>
        <Label fx:id="titleLbl" text="Überschrift Übersicht oder so" BorderPane.alignment="CENTER">
            <font>
                <Font size="33.0" />
            </font></Label>
    </top>
    <center>
        <GridPane alignment="TOP_CENTER" hgap="15.0" vgap="15.0" BorderPane.alignment="CENTER">
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
            </rowConstraints>
            <columnConstraints>
                <ColumnConstraints />
                <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <padding>
                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
            </padding>
            <children>
                <Label fx:id="jobLbl" text="Ausbildungsberuf" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <ComboBox fx:id="jobChoiceBox" prefHeight="25.0" prefWidth="329.0" GridPane.columnIndex="2" GridPane.columnSpan="2147483647" GridPane.rowIndex="1" />
                <Label fx:id="yearLbl" text="Ausbildungsjahr" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <ComboBox fx:id="yearChoiceBox" prefHeight="25.0" prefWidth="20.0" GridPane.columnIndex="2" GridPane.columnSpan="2147483647" GridPane.rowIndex="2" />

                <Button fx:id="backBtn" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="214.0" text="Zurück" GridPane.columnIndex="2" GridPane.columnSpan="1" GridPane.rowIndex="6" />
                <Button fx:id="savePDFBtn" mnemonicParsing="false" onAction="#savePDF" prefHeight="25.0" prefWidth="214.0" text="Als PDF speichern" GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.rowIndex="6" />
            </children>
        </GridPane>
   </center>
    <right>
        <GridPane fx:id="rightGrid" alignment="TOP_CENTER" hgap="15.0" vgap="15.0" BorderPane.alignment="CENTER">
            <children>
            <Label fx:id="configLbl" text="Konfiguration" GridPane.columnIndex="1" GridPane.columnSpan="2147483647" GridPane.rowIndex="5" />
            <CheckBox fx:id="firstCbx" mnemonicParsing="false" text="CheckBox" GridPane.columnIndex="1" GridPane.rowIndex="9" />
            <CheckBox fx:id="secondCbx" mnemonicParsing="false" text="CheckBox" GridPane.columnIndex="1" GridPane.rowIndex="10" />
            <CheckBox fx:id="thirdCbx" mnemonicParsing="false" text="CheckBox" GridPane.columnIndex="1" GridPane.rowIndex="11" />
            <CheckBox fx:id="fourthCbx" mnemonicParsing="false" text="CheckBox" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <CheckBox fx:id="fifthCbx" mnemonicParsing="false" text="CheckBox" GridPane.columnIndex="1" GridPane.rowIndex="7" />
            <CheckBox fx:id="sixthCbx" mnemonicParsing="false" text="CheckBox" GridPane.columnIndex="1" GridPane.rowIndex="8" />
